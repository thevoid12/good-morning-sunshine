npx tailwindcss -c ./client/ui/tailwind.config.js -i ./client/ui/assets/css/input.css -o ./client/ui/assets/css/output.css

npx tailwindcss -i ./client/ui/assets/css/input.css -o ./client/ui/assets/css/output.css --watch

